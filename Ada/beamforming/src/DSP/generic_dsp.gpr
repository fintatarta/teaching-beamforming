project Generic_Dsp is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Main use ("main.adb");
   for Exec_Dir use "bin";

   package Ide is
      for Documentation_Dir use "doc";
   end Ide;

   package Compiler is
      for Switches ("ada") use ("-gnatwa", "-gnata", "-g", "-gnato", "-fstack-check");
   end Compiler;

end Generic_Dsp;

